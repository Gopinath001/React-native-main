{
  "name": "rap",
  "private": true,
  "version": "1.0.0",
  "description": "The RAP frontend repo",
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "cd ios && pod install && cd .. && react-native run-ios --simulator=\"iPhone 12 Pro\"",
    "start": "npx react-native start",
    "------------CODE QUALITY": "------------",
    "test": "yarn jest --collectCoverage --passWithNoTests",
    "lint": "eslint",
    "-----------TOOLS": "------------",
    "webpack": "NODE_FLAGS='-r ./src/env.ts' webpack --mode",
    "components": "start-storybook -p 7007",
    "------------APPS": "------------",
    "gen-graphql": "yarn graphql-codegen --config codegen.yml",
    "webserver": "yarn ts-node --project ./src/server/tsconfig.json -r tsconfig-paths/register -r ./src/env.ts ./src/server/index.tsx",
    "serve": "concurrently -c blue,yellow,green -n Bundles,Server 'yarn webpack development' 'nodemon --watch ./src/web/ -e ts,tsx,js,jsx --exec yarn webserver'",
    "mobile": "yarn react-native",
    "build-theme": "style-dictionary build",
    "------------HOOKS AND UTILS": "------------",
    "postinstall": "yarn ts-node ./ops/setup.ts && husky install",
    "------------GRAVEYARD": "---------------",
    "build": "echo \n\n\"⚠⚠ This command is deprecated in favour of more specific commands like 'yarn build-web' or 'yarn mobile run-android'⚠⚠\" exit 1",
    "web": "echo \n\n\"⚠⚠️ This command is now deprecated, if you want to run a web dev server run 'yarn serve'⚠⚠\"; exit 1"
  },
